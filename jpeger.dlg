DLGTEMPLATE WND_COMPRESSION LOADONCALL MOVEABLE DISCARDABLE
BEGIN
    DIALOG  "JPEG Compression", WND_COMPRESSION, -150, -51, 296, 88, 
            FS_NOMOVEWITHOWNER | FS_MOUSEALIGN, FCF_TITLEBAR | 
            FCF_NOBYTEALIGN
            PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
            PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
            PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
            PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
            PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
            PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
            PRESPARAMS PP_BORDERCOLOR, 0x00000000L
            PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
    BEGIN
        AUTORADIOBUTTON "Compress using slo~w integer method", 
                        RB_COMPRESS_SLOW_INTEGER, 6, 75, 128, 8, WS_GROUP | 
                        WS_TABSTOP
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        AUTORADIOBUTTON "Compress using fas~t integer method", 
                        RB_COMPRESS_FAST_INTEGER, 6, 67, 125, 8
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        AUTORADIOBUTTON "Compress using ~float method", RB_COMPRESS_FLOAT, 6, 
                        59, 104, 8
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        AUTOCHECKBOX    "Use ~Huffman optimization encoding", 
                        CK_HUFFMAN_OPTIMIZATION, 6, 49, 125, 8, WS_GROUP
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        LTEXT           "~Quality:", -1, 6, 37, 24, 8, DT_MNEMONIC | NOT 
                        WS_GROUP
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        SLIDER          SL_QUALITY, 43, 35, 231, 14, SLS_SNAPTOINCREMENT | 
                        SLS_BUTTONSLEFT | SLS_RIBBONSTRIP
                        CTLDATA 12, 0, 101, 0, 101, 0
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x0080C0A0L
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        ENTRYFIELD      "", EF_QUALITY, 277, 39, 12, 6, ES_CENTER | 
                        ES_MARGIN | ES_READONLY | NOT WS_TABSTOP
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        LTEXT           "~Smoothing:", -1, 6, 23, 36, 8, DT_MNEMONIC | NOT 
                        WS_GROUP
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        SLIDER          SL_SMOOTHING, 43, 21, 231, 14, SLS_SNAPTOINCREMENT | 
                        SLS_BUTTONSLEFT | SLS_RIBBONSTRIP
                        CTLDATA 12, 0, 101, 0, 101, 0
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x0080C0A0L
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        ENTRYFIELD      "", EF_SMOOTHING, 277, 25, 12, 6, ES_CENTER | 
                        ES_MARGIN | ES_READONLY | NOT WS_TABSTOP
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        AUTOCHECKBOX    "Force ~baseline", CK_FORCE_BASELINE, 6, 11, 60, 8
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        AUTOCHECKBOX    "~Progressive JPEG", CK_PROGRESSIVE_JPEG, 6, 1, 71, 
                        8, NOT WS_TABSTOP
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        DEFPUSHBUTTON   "Rel~oad", DID_OK, 211, 1, 40, 14, WS_GROUP
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
        PUSHBUTTON      "~Cancel", DID_CANCEL, 251, 1, 40, 14, NOT 
                        WS_TABSTOP
                        PRESPARAMS PP_FOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_BACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_HILITEFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_HILITEBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_DISABLEDFOREGROUNDCOLOR, 0x00000000L
                        PRESPARAMS PP_DISABLEDBACKGROUNDCOLOR, 0x00CCCCCCL
                        PRESPARAMS PP_BORDERCOLOR, 0x00000000L
                        PRESPARAMS PP_FONTNAMESIZE, "8.Helv"
    END
END

