    DIALOG  "JPEGER", WND_MOODIES, -85, -60, 164, 104, FS_NOMOVEWITHOWNER | 
            FS_MOUSEALIGN | WS_VISIBLE, FCF_SYSMENU | FCF_TITLEBAR | 
